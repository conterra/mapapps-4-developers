{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "*": ["./src/main/js/bundles/*", "./node_modules/@conterra/ct-mapapps-typings/*"]
        },
        "allowJs": true,
        "noEmit": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": ["src"]
}
